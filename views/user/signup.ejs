    <input type="password" class="form-control rounded-0 height-4 px-4" name="password1" id="signupPword1" placeholder="" aria-label="" aria-describedby="signinPasswordLabel9" >
    <p style="display: none; color: red;"    id="msgThree">This feild is required</p>
    </div>
    </div>


  
    <div class="form-group mb-4">
      <div class="js-form-message js-focus-state">
      <label id="signinPasswordLabel9" class="form-label" for="signinPassword9">Confirm password *</label>
      <input type="password" class="form-control rounded-0 height-4 px-4" name="password2" id="signupPword2" placeholder="" aria-label="" aria-describedby="signinPasswordLabel9" >
      <p style="display: none;" style="color: red;" id="msgFour">This feild is required</p>
      <p style="display: none;" id="cnp">Password must be same</p>
      </div>
      </div>
  


    <div class="form-group mb-4">
      <div class="js-form-message js-focus-state">
      <label id="signinPasswordLabel9" class="form-label" for="signinPassword9">Mobile number*</label>
      <input type="text" class="form-control rounded-0 height-4 px-4" name="mbno" id="signupmbno" placeholder="" aria-label="" aria-describedby="signinPasswordLabel9">
      <p style="display: none;" style="color: red;" id="msgFive">This feild is required</p>
      
      <p style="color:red ;"><%= msgTwo%></p>
      </div>
      </div>


