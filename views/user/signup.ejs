<script>
  document.getElementById("submit").addEventListener('click',(event)=>{
    // event.preventDefault()
    console.log('checking');
   const username = document.getElementById('signupname').value
   const email = document.getElementById('signupemail').value
   const password1 = document.getElementById('signupPword1').value
   const confmPwrod = document.getElementById('signupPword2').value
   const mobilenumber = document.getElementById('signupmbno').value
   msgOne.style.display="none"
   msgTwo.style.display='none'
   msgThree.style.display='none'
   cnp.style.display='none'
   msgFive.style.display='none'


 if(username==''){
    event.preventDefault()
    msgOne.style.display="block"
   }
 else if(email==""){
   event.preventDefault()
   msgTwo.style.display='block'
}
else if(password1==""){
  event.preventDefault()
  msgThree.style.display='block'
}
else if(confmPword==""){
  event.preventDefault()
  msgFour.style.display='block'
}
else if(mobilenumber==""){
  event.preventDefault()
  msgFive.style.display='block'
}

})
</script>
